
<table>
  <th>Rubric ID</th>
  <th>Title</th>
  <th>Products_Count</th>

  <% @arecord_result.each { |r| %>
      <tr>
        <td><%= r.rubric_id %></td>
        <td><%= r.title %></td>
        <td><%= r.products_count %></td>
      </tr>
  <% } %>
</table>
